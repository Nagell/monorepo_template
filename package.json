{
    "name": "monorepo_template",
    "version": "1.0.0",
    "private": true,
    "workspaces": {
        "packages": [
            "apps/*",
            "libs/*"
        ]
    },
    "scripts": {
        "init": "yarn install",
        "ci": "yarn install --frozen-lockfile",
        "build:prod": "nx run-many -t build:prod",
        "build:staging": "nx run-many -t build:staging",
        "clean": "npm exec --workspaces -- npx rimraf node_modules && npx rimraf node_modules"
    },
    "dependencies": {
        "@vueuse/core": "^10.7.1",
        "@vueuse/integrations": "^10.11.0",
        "axios": "^1.6.4",
        "pinia": "^2.1.7",
        "sass": "^1.69.7",
        "vue": "^3.4.5",
        "vue-i18n": "^9.8.0",
        "vue-router": "^4.3.2"
    },
    "devDependencies": {
        "@html-eslint/eslint-plugin": "^0.26.0",
        "@html-eslint/parser": "^0.26.0",
        "@nx/eslint": "18.3.2",
        "@nx/eslint-plugin": "18.3.2",
        "@nx/js": "18.3.2",
        "@nx/storybook": "18.3.2",
        "@nx/vite": "18.3.2",
        "@nx/vue": "18.3.2",
        "@nx/web": "18.3.2",
        "@stylistic/eslint-plugin": "^1.6.3",
        "@swc-node/register": "~1.9.0",
        "@swc/core": "~1.4.16",
        "@swc/helpers": "~0.5.2",
        "@types/node": "^20.11.18",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vue/eslint-config-typescript": "^13.0.0",
        "eslint": "^8.56.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-vue": "^9.19.2",
        "nx": "18.3.2",
        "ts-node": "10.9.2",
        "tslib": "^2.3.0",
        "typescript": "~5.4.5",
        "vite": "^5.1.3",
        "vite-plugin-chunk-split": "^0.5.0"
    }
}